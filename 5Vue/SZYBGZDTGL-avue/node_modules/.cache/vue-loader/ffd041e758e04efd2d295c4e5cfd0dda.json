etHeight / 3) {\r\n        this.selectTab = 0\r\n      }\r\n      if (e.target.scrollTop >= this.$refs.first.offsetHeight / 3 && e.target.scrollTop < (this.$refs.first.offsetHeight + this.$refs.second.offsetHeight/3)) {\r\n        this.selectTab = 1\r\n      }\r\n      if (e.target.scrollTop >= (this.$refs.second.offsetHeight/3 + this.$refs.first.offsetHeight)) {\r\n        this.selectTab = 2\r\n      }\r\n    },\r\n    loadData(ref) {\r\n      // Set the block to loading state\r\n      this.$refs[ref].stateLoading()\r\n\r\n      // .. here you could load your data\r\n\r\n      // Set a timeout for demo purposes\r\n      setTimeout(() => {\r\n        // Set the block back to normal state\r\n        this.$refs[ref].stateNormal()\r\n      }, 2000)\r\n    },\r\n    testOnChange(data) {\r\n      console.log(\"testOnChange::::\" + data);\r\n    },\r\n\r\n    testOnInput(data) {\r\n      console.log(\"testOnInput::\" + data);\r\n    },\r\n    goNavList(val) {\r\n      let that = this\r\n      switch (val) {\r\n        case 0:\r\n          that.height = 0;\r\n          that.selectTab = 0\r\n          break;\r\n        case 1:\r\n          that.height = that.$refs.first.offsetHeight;\r\n          that.selectTab = 1\r\n          break;\r\n        case 2:\r\n          that.height = that.$refs.first.offsetHeight + that.$refs.second.offsetHeight;\r\n          that.selectTab = 2\r\n          break;\r\n        default:\r\n          break;\r\n      }\r\n      that.$refs.orderview.scrollTop = that.height;\r\n    },\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n\r\n\r\n</style>"]}]}