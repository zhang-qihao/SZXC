    "babel-polyfill": "^6.26.0",
    "classlist-polyfill": "^1.2.0",
    "crypto-js": "^4.0.0",
    "echarts": "^5.0.2",
    "element-ui": "^2.4.5",
    "jquery": "3.5.1",
    "js-cookie": "^2.2.0",
    "js-md5": "^0.7.3",
    "mockjs": "^1.0.1-beta3",
    "nprogress": "^0.2.0",
    "print-js": "^1.6.0",
    "qrcodejs2": "0.0.2",
    "script-loader": "^0.7.2",
    "sortablejs": "^1.10.0-rc2",
    "vue": "^2.5.16",
    "vue-axios": "^2.1.2",
    "vue-codemirror": "^4.0.6",
